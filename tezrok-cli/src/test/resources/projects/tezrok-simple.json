{
  "name": "simple-app",
  "version": "0.0.1",
  "package": "com.tezrok.simple",
  "modules": [
    {
      "name": "simpleModule",
      "type": "tezrok",
      "schema": {
        "schemaName": "smpl",
        "importSchema": "./schemas/Orders.json",
        "entities": [
          {
            "name": "Order",
            "customRepository": true,
            "fields": [
              {
                "name": "id",
                "type": "long",
                "primary": true
              },
              {
                "name": "name",
                "unique": true
              },
              {
                "name": "otherItems",
                "relation": "ManyToMany"
              },
              {
                "name": "active",
                "type": "boolean",
                "required": true
              }
            ]
          },
          {
            "name": "Item",
            "customRepository": true,
            "customMethods": [
              "findOrderItemsByItemItemsOrderIdAndItemTitle",
              "findOrderOtherItemsByOrderIdAndItemTitle"
            ],
            "fields": [
              {
                "name": "id",
                "type": "long",
                "primary": true
              },
              {
                "name": "otherItems",
                "type": "Item",
                "relation": "ManyToMany"
              }
            ]
          }
        ]
      },
      "auth": {
        "type": "simple"
      },
      "dependencies": [
        "commons-io:commons-io:2.13.0"
      ]
    }
  ]
}
